jellyfin

  docker run -d
--name jellyfin
-v /opt/docker/jellyfin/config:/config
-v /opt/docker/jellyfin/cache:/cache
-v /mnt/sdc1:/media
-e TZ="Asia/Shanghai"
-p 6000:6000
--device=/dev/dri/renderD128
--restart unless-stopped
jellyfin/jellyfin

 aliyun
 docker run -d 
 --name=aliyundrive-webdav 
 --restart=unless-stopped 
 -p 6001:6001 
 -e REFRESH_TOKEN='410accfcf94e4e47a037caf792f9948f' 
 messense/aliyundrive-webdav

music-player
 
 docker run -d 
 --name music-player
 -p 6002:6002 
 -v /tmp/music:/var/www/html/cache 
 oldiy/music-player-docker

baidunetdisk

 docker create  \
    --name=baidunetdisk  \
    -p 5800:5800  \
    -p 5900:5900  \
    -v /opt/docker/baidunetdisk:/config  \
    -v /mnt/sdc1/download:/config/baidunetdiskdownload  \
    -e VNC_PASSWORD=366454264
    --restart unless-stopped  \
    johngong/baidunetdisk:latest
    
xteve

docker run -d \
  --name=xteve_guide2go \
  --net=host \
  --log-opt max-size=10m \
  --log-opt max-file=3 \
  -e TZ="Asia/Shanghai" \
  -v /opt/docker/xteve/:/root/.xteve:rw \
  -v /opt/docker/xteve/_config/:/config:rw \
  -v /opt/docker/xteve/_guide2go/:/guide2go:rw \
  -v /tmp/xteve/:/tmp/xteve:rw \
  -v /opt/docker/tvheadend/data/:/TVH \
  -p 6004:6004
  alturismo/xteve_guide2go

Homeassistant

docker run --init -d
 --name="HA"
  -v /mnt/sda3/docker/hass:/config
   -v /etc/localtime:/etc/
   localtime:ro 
   -p 8123:8123 
   homeassistant/home-assistant
